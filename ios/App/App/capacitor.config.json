{
	"appId": "com.resgrid.unit",
	"appName": "Resgrid Unit",
	"webDir": "www",
	"bundledWebRuntime": false,
	"android": {
		"allowMixedContent": true,
		"useLegacyBridge": true
	},
	"server": {
		"allowNavigation": []
	},
	"plugins": {
		"PushNotifications": {
			"presentationOptions": [
				"badge",
				"sound",
				"alert"
			]
		},
		"SplashScreen": {
			"launchAutoHide": false
		},
		"Badge": {
			"persist": false,
			"autoClear": true
		},
		"CapacitorHttp": {
			"enabled": true
		}
	},
	"packageClassList": [
		"BackgroundGeolocation",
		"BluetoothLe",
		"FileOpenerPlugin",
		"KeepAwakePlugin",
		"CapacitorSQLitePlugin",
		"AppPlugin",
		"CAPCameraPlugin",
		"DevicePlugin",
		"FilesystemPlugin",
		"GeolocationPlugin",
		"HapticsPlugin",
		"KeyboardPlugin",
		"CAPNetworkPlugin",
		"PreferencesPlugin",
		"SplashScreenPlugin",
		"StatusBarPlugin",
		"BadgePlugin",
		"LocalNotificationsPlugin",
		"PushNotificationsPlugin",
		"ResgridPlugin"
	]
}
