{
	"appId": "com.resgrid.unit",
	"appName": "Resgrid Unit",
	"webDir": "www",
	"bundledWebRuntime": false,
	"android": {
		"allowMixedContent": true,
		"useLegacyBridge": true
	},
	"server": {
		"allowNavigation": []
	},
	"plugins": {
		"PushNotifications": {
			"presentationOptions": [
				"badge",
				"sound",
				"alert"
			]
		},
		"SplashScreen": {
			"launchAutoHide": false
		},
		"Badge": {
			"persist": false,
			"autoClear": true
		},
		"CapacitorHttp": {
			"enabled": true
		}
	}
}
